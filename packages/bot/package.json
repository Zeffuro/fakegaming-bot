{
  "name": "@zeffuro/fakegaming-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "description": "",
  "author": "Zeffuro",
  "license": "AGPL-3.0-only",
  "scripts": {
    "prebuild": "tsx ../../scripts/generate-bot-manifest.ts && pnpm -C ../../packages/common build",
    "build": "pnpm --filter @zeffuro/fakegaming-common run build && tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "prod:dev": "cross-env NODE_ENV=production tsx src/index.ts",
    "start:dev": "cross-env NODE_ENV=development tsx src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --ext .ts --config ../../eslint.config.mjs",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "profile": "node --inspect-brk dist/index.js"
  },
  "dependencies": {
    "@zeffuro/fakegaming-common": "workspace:*",
    "axios": "^1.13.5",
    "canvas": "^3.2.1",
    "cheerio": "^1.2.0",
    "cross-env": "^10.1.0",
    "discord-api-types": "^0.38.39",
    "discord.js": "^14.25.1",
    "ms": "^2.1.3",
    "sqlite3": "^5.1.7",
    "sequelize": "^6.37.7",
    "twisted": "^1.72.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/ms": "^2.1.0",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "pino-pretty": "^13.1.3"
  }
}