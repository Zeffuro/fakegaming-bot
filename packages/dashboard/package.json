{
  "name": "@zeffuro/fakegaming-bot-dashboard",
  "version": "1.0.0",
  "description": "Dashboard for Fakegaming.eu Bot.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "Zeffuro",
  "license": "AGPL-3.0-only",
  "scripts": {
    "build": "next build",
    "prebuild": "pnpm -w run gen:api-types",
    "prepare": "pnpm -w run gen:api-types",
    "start": "next start",
    "start:dev": "pnpm --filter @zeffuro/fakegaming-bot-api export:openapi && pnpm -w run gen:api-types && next dev",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint app components lib --ext .ts,.tsx --config ../../eslint.config.mjs",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@zeffuro/fakegaming-common": "workspace:*",
    "better-auth": "^1.4.15",
    "cross-env": "^10.1.0",
    "discord-api-types": "^0.38.37",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "next": "^16.1.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "pino-pretty": "^13.1.3"
  }
}