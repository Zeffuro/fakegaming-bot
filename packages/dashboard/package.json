{
  "name": "@zeffuro/fakegaming-bot-dashboard",
  "version": "1.0.0",
  "description": "Dashboard for Fakegaming.eu Bot.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "Zeffuro",
  "license": "AGPL-3.0-only",
  "scripts": {
    "build": "next build",
    "prebuild": "pnpm run generate:api-types",
    "prepare": "pnpm run generate:api-types",
    "generate:api-aliases": "pnpm tsx scripts/generateApiAliases.ts",
    "start": "next start",
    "start:dev": "pnpm --filter @zeffuro/fakegaming-bot-api export:openapi && pnpm generate:api-types && next dev",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint app components lib --ext .ts,.tsx --config ../../eslint.config.mjs",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "generate:api-types": "openapi-typescript ../api/openapi.json -o types/api.d.ts && pnpm tsx scripts/generateApiAliases.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@zeffuro/fakegaming-common": "workspace:*",
    "better-auth": "^1.3.27",
    "cross-env": "^10.1.0",
    "discord-api-types": "^0.38.30",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.5.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "jsdom": "^27.0.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "pino-pretty": "^13.1.2"
  }
}