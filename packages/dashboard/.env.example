# Dashboard port and public URL
PORT=3000
PUBLIC_URL=http://localhost:3000

# Discord Credentials
DISCORD_CLIENT_ID=xxxxxxxxxxxxxxxxxxxx
DISCORD_CLIENT_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
DISCORD_BOT_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# URL of the API the dashboard connects to
API_URL=http://localhost:3001/api

# OAuth2 client credentials for the dashboard to the API
DASHBOARD_CLIENT_ID=dashboard
DASHBOARD_CLIENT_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Discord users with admin access to the dashboard
DASHBOARD_ADMINS=xxxxxxxxxxxxxxxxxxxxxxxx

# Redis Database URL for session storage (if not used defaults to in-memory storage)
# Redis has a free tier at https://redis.com/try-free/
REDIS_URL=redis://default:password@redis-host:6379