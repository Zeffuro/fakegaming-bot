{
  "name": "@zeffuro/fakegaming-common",
  "version": "1.0.0",
  "description": "Common database models and utilities for FakeGaming projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "Zeffuro",
  "license": "AGPL-3.0-only",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./models": {
      "import": "./dist/models/index.js",
      "types": "./dist/models/index.d.ts"
    },
    "./managers": {
      "import": "./dist/managers/index.js",
      "types": "./dist/managers/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./discord": {
      "import": "./dist/discord/index.js",
      "types": "./dist/discord/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./utils/cache": {
      "import": "./dist/utils/cacheManager.js",
      "types": "./dist/utils/cacheManager.d.ts"
    },
    "./auth": {
      "import": "./dist/discord/auth.js",
      "types": "./dist/discord/auth.d.ts"
    },
    "./testing": {
      "import": "./dist/testing/index.js",
      "types": "./dist/testing/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.d.ts --config ../../eslint.config.mjs",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.8",
    "discord.js": "^14.3.1",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "ioredis": "^5.8.0",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "umzug": "^3.8.2",
    "zod": "^4.1.12",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}