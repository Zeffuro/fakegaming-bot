  },
  "dependencies": {
    "@zeffuro/fakegaming-common": "workspace:*",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.4"
  }
}