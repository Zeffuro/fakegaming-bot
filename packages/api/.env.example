# API port and public URL
PORT=3001
PUBLIC_URL=http://localhost:3001/api

# Database credentials, use SQLite for easy local development, can omit database url for SQLite
# When running with Docker Compose, DATABASE_URL is set automatically from the root .env variables.
DATABASE_PROVIDER=postgres
DATABASE_URL="postgres://fakegaming:xxxxxxxxxxxxxxxxx@xxxxx.xxxxxxxxxxx.xxx:5432/fakegaming"

# Secret used to sign JWT tokens
JWT_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# OAuth2 client credentials for the dashboard to the API
DASHBOARD_CLIENT_ID=dashboard
DASHBOARD_CLIENT_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx